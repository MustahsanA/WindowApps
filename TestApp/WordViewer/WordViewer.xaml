<UserControl x:Class="Meddiff.Common.WordViewer.WordViewer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:Meddiff.Common.WordViewer="clr-namespace:Meddiff.Common.WordViewer" mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300"
             >
    <DockPanel LastChildFill="True" ClipToBounds="True" >
        <WindowsFormsHost x:Name="wordHost">
            <Meddiff.Common.WordViewer:MSWordHost>
                
            </Meddiff.Common.WordViewer:MSWordHost>
        </WindowsFormsHost>
    </DockPanel>
</UserControl>
